local function animate(trm, x,y, txt)\
  trm.setCursorPos(x,y)\
  trm.setTextColor(colors.white)\
  trm.setBackgroundColor(colors.gray)\
  trm.write(\" \"..txt..\" \")\
  sleep(0.1)\
  trm.setCursorPos(x,y)\
  trm.setTextColor(colors.gray)\
  trm.setBackgroundColor(colors.white)\
  trm.write(\" \"..txt..\" \")\
end\
