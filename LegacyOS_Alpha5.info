local function strToTable(str)\
  local tbl = {}\
  local wrd = \"\"\
  for i=1,#str do\
    local chr = string.sub(str,i,i)\
    if chr == \" \" then\
      tbl[#tbl+1] = wrd\
      wrd = \"\"\
    else\
      wrd = wrd..chr\
    end\
  end\
  tbl[#tbl+1] = wrd\
  return tbl\
end\
